{
  "name": "halspan-web-application",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "test": "react-scripts test",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "eslint": "eslint src/**/*.js* --fix",
    "bdd_dry_run": "./node_modules/@cucumber/cucumber/bin/cucumber.js --dry-run test --require test",
    "preview": "vite preview",
    "generate_report": "nyc report --temp-dir ./test/step-definations/coverageData --reporter html --report-dir ./coverage",
    "code_coverage": "nyc check-coverage --temp-dir ./test/step_definitions/coverageData",
    "owasp": "owasp-dependency-check --out=./dependency-check-reports --project Halspan -f JSON -f HTML --prettyPrint --failOnCVSS 8 --data=$(node -p \"require('os').homedir() + '/dependency-check-data'\") --noupdate --scan=package-lock.json --log=logs.txt && cat logs.txt",
    "generate_report_cobertura": "nyc report --temp-dir ./test/step-definations/coverageData --reporter cobertura --report-dir ./coverage-cobertura",
    "cucumber": "npx cucumber-js --import test"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@aws-sdk/client-s3": "^3.645.0",
    "@aws-sdk/s3-request-presigner": "^3.645.0",
    "@cucumber/cucumber": "^10.8.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.7.0",
    "@mui/x-date-pickers": "^7.20.0",
    "@reduxjs/toolkit": "^2.2.6",
    "@uiw/react-signature": "^1.3.1",
    "antd": "^5.21.4",
    "axios": "^1.7.2",
    "babel-plugin-istanbul": "^5.1.0",
    "cucumber": "^6.0.7",
    "cucumber-html-reporter": "^7.2.0",
    "dayjs": "^1.11.13",
    "detect-secrets": "^1.0.6",
    "dotenv": "^16.4.5",
    "eslint": "^9.7.0",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-security": "^3.0.1",
    "express": "^4.19.2",
    "framer-motion": "^11.3.2",
    "istanbul": "^0.4.5",
    "istanbul-lib-coverage": "^3.2.0",
    "jsonwebtoken": "^9.0.2",
    "license-checker": "^25.0.1",
    "localforage": "^1.10.0",
    "moment": "^2.30.1",
    "papaparse": "^5.4.1",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.3.1",
    "react-draggable-list": "^4.2.1",
    "react-image-marker": "^1.2.0",
    "react-redux": "^9.1.2",
    "react-responsive-modal": "^6.4.2",
    "react-router-dom": "^6.24.1",
    "react-signature-canvas": "^1.0.6",
    "react-sketch-canvas": "^6.2.0",
    "react-spinners": "^0.14.1",
    "react-time-picker": "^7.0.0",
    "react-zoom-pan-pinch": "^3.6.1",
    "redux-persist": "^6.0.0",
    "regex-validator": "^0.0.1-alpha.0",
    "regexpp": "^3.2.0",
    "selenium-webdriver": "^4.22.0",
    "uuid": "^10.0.0",
    "vite-plugin-istanbul": "^6.0.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "globals": "^15.8.0",
    "nyc": "^17.0.0",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.5",
    "vite": "^5.3.1",
    "vite-plugin-istanbul": "^5.0.0"
  }
}
